    "electron-canvas-to-buffer": "^2.0.0",
    "electron-google-analytics": "^0.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-localshortcut": "^3.1.0",
    "electron-store": "^2.0.0",
    "i18n": "^0.8.3",
    "materialize-css": "^1.0.0",
